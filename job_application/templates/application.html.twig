{#
/**
 * @file application.html.twig
 * Default theme implementation to present Application data.
 *
 * This template is used when viewing Application pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_application()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('application') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
